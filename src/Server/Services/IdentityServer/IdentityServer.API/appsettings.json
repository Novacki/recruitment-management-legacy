{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "Default": "Server=(localdb)\\DEVELOPMENT;Initial Catalog=IdentityServer;Integrated Security=True;Connect Timeout=30;Encrypt=False;TrustServerCertificate=False;ApplicationIntent=ReadWrite;MultiSubnetFailover=False;"
  },
  "AppSettings": {
    "AuthSettings": {
      "CookieSettings": {
        "LoginPath": "/auth/signin",
        "LogoutPath": "/auth/signout",
        "AccessDeniedPath": "/auth/signin",
        "CookieName": "IdentityServerAuthentication",
        "ReturnUrlParameter": "returnUrl",
        "ExpireTimeSpanMinutes": 60,
        "HttpOnly": true,
        "SlidingExpiration": false
      }
    }
  },
  "AllowedHosts": "*"
}
